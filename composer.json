{
  "name": "pnixx/amp-memcache",
  "description": "A non-blocking Memcache client for Amp.",
  "keywords": [
    "amp",
    "amphp",
    "async",
    "client",
    "memcache"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Odintsov",
      "email": "nixx.dj@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "amphp/amp": "^3.0",
    "amphp/socket": "^2.0",
    "revolt/event-loop": "^1.0",
    "psr/log": "^3.0",
    "ext-ctype": "*"
  },
  "require-dev": {
    "amphp/phpunit-util": "^3",
    "phpunit/phpunit": "^9"
  },
  "minimum-stability": "beta",
  "autoload": {
    "psr-4": {
      "PNixx\\Memcache\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PNixx\\Memcache\\Test\\": "test"
    }
  }
}
